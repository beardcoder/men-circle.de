{
  "name": "mens-circle/site",
  "description": "Website https://mens-circle.de",
  "license": "GPL-2.0-or-later",
  "type": "project",
  "require": {
    "b13/container": "dev-master",
    "b13/picture": "dev-master",
    "friendsoftypo3/content-blocks": "^1.0",
    "helhum/dotenv-connector": "^3.1",
    "helhum/typo3-console": "^8.2",
    "lcobucci/clock": "^3.3",
    "lcobucci/jwt": "^5.4",
    "lochmueller/staticfilecache": "^15.0",
    "mens-circle/sitepackage": "@dev",
    "praetorius/vite-asset-collector": "^1.11",
    "spatie/icalendar-generator": "^2.8.1",
    "spatie/schema-org": "^3.23",
    "typo3/cms-adminpanel": ">=13.3",
    "typo3/cms-beuser": ">=13.3",
    "typo3/cms-form": ">=13.3",
    "typo3/cms-impexp": ">=13.3",
    "typo3/cms-install": ">=13.3",
    "typo3/cms-linkvalidator": ">=13.3",
    "typo3/cms-reactions": ">=13.3",
    "typo3/cms-redirects": ">=13.3",
    "typo3/cms-reports": ">=13.3",
    "typo3/cms-rte-ckeditor": ">=13.3",
    "typo3/cms-scheduler": ">=13.3",
    "typo3/cms-seo": ">=13.3",
    "typo3/cms-setup": ">=13.3",
    "typo3/cms-t3editor": ">=13.3",
    "typo3/cms-tstemplate": ">=13.3",
    "typo3/minimal": ">=13",
    "vlucas/phpdotenv": "^5.6"
  },
  "require-dev": {
    "b13/make": "dev-main",
    "ergebnis/composer-normalize": "^2.44",
    "phpro/grumphp-shim": "^2.6",
    "roave/security-advisories": "dev-latest",
    "ssch/typo3-rector": "dev-main",
    "symfony/var-dumper": "^7.1",
    "symplify/easy-coding-standard": "^12.5",
    "typo3/cms-belog": ">=13.3",
    "typo3/cms-lowlevel": ">=13.3",
    "typo3/cms-styleguide": "^13.4",
    "typo3/coding-standards": "^0.8.0",
    "vimeo/psalm": "^6.0@dev"
  },
  "repositories": [
    {
      "type": "path",
      "url": "./packages/*/"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "helhum/dotenv-connector": true,
      "phpro/grumphp-shim": true,
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true
    },
    "platform": {
      "php": "8.4"
    },
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  }
}
