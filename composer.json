{
	"name": "menscircle/site",
	"description": "TYPO3 CMS Base Distribution",
	"license": "GPL-2.0-or-later",
	"type": "project",
	"require": {
		"b13/container": "dev-master",
		"b13/picture": "dev-master",
		"contentblocks/content-blocks": "^0.8",
		"helhum/dotenv-connector": "^3.1",
		"mens-circle/sitepackage": "@dev",
		"praetorius/vite-asset-collector": "^1.10",
		"spatie/icalendar-generator": "^2.8",
		"spatie/schema-org": "^3.23",
		"typo3/cms-adminpanel": ">=13.2",
		"typo3/cms-beuser": ">=13.2",
		"typo3/cms-fluid-styled-content": ">=13.2",
		"typo3/cms-form": ">=13.2",
		"typo3/cms-impexp": ">=13.2",
		"typo3/cms-install": ">=13.2",
		"typo3/cms-linkvalidator": ">=13.2",
		"typo3/cms-reactions": ">=13.2",
		"typo3/cms-recycler": ">=13.2",
		"typo3/cms-redirects": ">=13.2",
		"typo3/cms-reports": ">=13.2",
		"typo3/cms-rte-ckeditor": ">=13.2",
		"typo3/cms-scheduler": ">=13.2",
		"typo3/cms-seo": ">=13.2",
		"typo3/cms-t3editor": ">=13.2",
		"typo3/cms-tstemplate": ">=13.2",
		"typo3/cms-viewpage": ">=13.2",
		"typo3/minimal": ">=13.1.0",
		"wyrihaximus/html-compress": "^4.3"
	},
	"require-dev": {
		"ergebnis/composer-normalize": "^2.43",
		"php-static-analysis/attributes": "^0.3.0",
		"php-static-analysis/rector-rule": "^0.3.0",
		"phpro/grumphp-shim": "^2.6",
		"symfony/var-dumper": "^7.1.2",
		"typo3/cms-belog": ">=13.2",
		"typo3/cms-lowlevel": ">=13.2",
		"typo3/cms-setup": ">=13.2",
		"typo3/coding-standards": "^0.8.0"
	},
	"repositories": [
		{
			"type": "path",
			"url": "./packages/*/"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"allow-plugins": {
			"ergebnis/composer-normalize": true,
			"helhum/dotenv-connector": true,
			"phpro/grumphp-shim": true,
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true
		},
		"platform": {
			"php": "8.3"
		},
		"sort-packages": true
	}
}
